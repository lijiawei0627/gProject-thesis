// 颜色定义规范
$color-background = #F5F5F7
$color-background-d = rgba(0, 0, 0, 0.1)
$color-background-l = rgba(0, 0, 0, 0.5)
$color-background-l = #FFF
//$color-highlight-background = rgba(229, 225, 225, 0.89)
$color-highlight-background = linear-gradient(to bottom, #eed6d6, #f5f5f7)
$color-highlight-background-l = #eed6d6
$color-highlight-background-r = #f5f5f7
$color-dialog-background = rgba(255, 255, 255, 0.77)
$color-theme = #94403d
$color-theme-l = #b84f4d
$color-theme-d = rgba(255, 205, 49, 0.5)
$color-sub-theme = #F08080
$color-text = #5c5c5c
$color-text-d = rgba(0, 0, 0, 0.3)
$color-text-l = rgba(0, 0, 0, 0.5)
$color-text-ll = rgba(0, 0, 0, 0.8)
$color-dots-l = rgba(255, 255, 255, 0.5)
$color-dots-ll = rgba(255, 255, 255, 0.8)
$color-dots-lll = rgba(255, 255, 255, 1)
//字体定义规范
$font-size-small-s = 10px
$font-size-small = 12px
$font-size-medium = 14px
$font-size-medium-x = 16px
$font-size-large = 18px
$font-size-large-x = 22px
